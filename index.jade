    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body
    .container-fluid.fondo1
        .container
            .row.menu
                .col-md-6.logo
                    img.img-responsive(src="img/pb2.png")
                .col-md-6
                    center
                    ul.list-unstyled.list-inline
                        li 
                            a.navbar(href="index.html") INICIO
                        li 
                            a(href="#sobremi") SOBRE MÍ
                        li 
                            a(href="#habilidades") HABILIDADES
                        li 
                            a(href="#portafolio") PORTFOLIO
                        li 
                            a(href="#contacto") CONTACTO
            .row.bienvenida
                h1.text-center.slideOutUp ¡BIENVENIDOS!
                h2.text-center "Te ofresco webs dinámicas, minimalistas y diseñadas para celulares."
        .container
            #sobremi
            .row.fondo2 
                h1.text-center PAOLA BEGAZO...
                hr.linea
                .col-md-6.foto
                    img.img-responsive(src="img/paola.jpg")
                .col-md-6.descript
                    p.text-center Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam,quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in.
                    .row.intereses
                        .col-md-3
                            div 
                                img(src="img/lap.png")
                        .col-md-3
                            div 
                                img(src="img/chef.png")
                        .col-md-3
                            div 
                                img(src="img/dog.png")
                        .col-md-3
                            div 
                                img(src="img/paz.png")
            #habilidades
            .row.fondo3
                h1.text-center MIS HABLIDADES
                hr.linea
                .col-md-6
                    img(src="img/html.png")
                    img(src="img/css.png")
                    img(src="img/boo.png")
                    img(src="img/sass.png")
                .col-md-6
                    img(src="img/wp.png")
                    img(src="img/angular.png")
                    img(src="img/js.png")
                    img(src="img/jade.png")
            #portafolio
            .row.fondo4
                h1.text-center PORTAFOLIO
                hr.linea
                .col-md-6
                    a(href="#")
                        img(src="img/akakau.png")
                    img(src="img/danza.png")
                    img(src="img/eco.png")
                .col-md-6
                    img(src="img/flat.png")
                    img(src="img/wpp.png")
                    img(src="img/primes.png")
<style type="text/css"> a.navbar:hover{color:red; font-weight:bold;background-image:url(http://www.nackvision.com/myspace/links/lightningline.gif);} a:hover, a.redlink:hover, a.navbar:hover {background-image:url(http://www.nackvision.com/myspace/links/lightningline.gif); text-decoration:none; border:0px solid;} a:hover img {background-image:url(http://www.nackvision.com/myspace/links/lightningline.gif); filter:alpha(finishopacity=0, style=2); } </style>

